<h2>Edit request</h2>

<%= render "form.html", changeset: @changeset,
                        action: request_path(@conn, :update, @request) %>

<%= link "Back", to: request_path(@conn, :users_view) , class: "btn btn-default btn-xm"%>



<div class="delete_div">
	<%= link "Delete", to: request_path(@conn, :delete, @request), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xm" %>
</div>



